{
  "entrypoint": "index.html",
  "shell": "src/space-wars-app/space-wars-app.html",
  "fragments": [
    "src/views/default-view.html",
    "src/views/game-view.html",
    "src/views/login-view.html",
    "src/views/space-view.html"
  ],
  "sources": [
